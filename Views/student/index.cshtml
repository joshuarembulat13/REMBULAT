<h2>Student List</h2>

<table border="1px">
   <tbody border="1px">
      <tr>
         <th>Student ID:</th>
         <td>@ViewBag.Student</td>
      </tr>
      <tr>
         <th>Name:</th>
         <td>@ViewBag.studentName</td>
      </tr>
      <tr>
         <th>Email:</th>
         <td>@ViewBag.studentEmail</td>
      </tr>
      <tr>
         <th>Course:</th>
         <td>@ViewBag.studentCourse</td>
      </tr>
      <tr>
         <th>Date Enrolled:</th>
         <td>@ViewBag.StudentDateEnrolled</td>
      </tr>
   </tbody>
</table>


